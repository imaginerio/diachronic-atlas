    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@turf/bbox": "^6.3.0",
    "d3": "^6.7.0",
    "lodash": "^4.17.21",
